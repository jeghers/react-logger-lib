  "author": "Mark Jeghers <mark.jeghers@gmail.com> (https://www.t4p.com/)",
  "bugs": {
    "url": "https://github.com/jeghers/react-logger-lib/issues"
  },
  "homepage": "https://github.com/jeghers/react-logger-lib#readme"
}
